    "@aws-sdk/lib-dynamodb": "^3.190.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "~4.16.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pm2": "^5.2.2"
  },
  "devDependencies": {
    "bootstrap": "^5.2.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "feather-icons": "^4.29.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0"
  }
