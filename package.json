{
  "name": "awsconnectdashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "nyc mocha --recursive --timeout 10000",
    "dev": "DEBUG=awsconnectdashboard:* ./node_modules/nodemon/bin/nodemon.js ./bin/www -e js,mjs,json,ejs,html,js,css",
    "start": "node ./bin/www",
    "pm-start": "./node_modules/pm2/bin/pm2 start bin/www --name awsconnectdashboard --time --log logs/logs.txt",
