{
  "name": "awsconnectdashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "nyc mocha --recursive --timeout 10000",
    "dev": "NODE_ENV=dev DEBUG=awsconnectdashboard:* ./node_modules/nodemon/bin/nodemon.js ./bin/www -e js,mjs,json,ejs,html,js,css",
    "create-first-user": "DEBUG=awsconnectdashboard:* node ./utilities/create_first_user.js",
    "dev-ndf": "NODE_ENV=dev DEV_NO_DATE_FILTER=1 DEBUG=awsconnectdashboard:* ./node_modules/nodemon/bin/nodemon.js ./bin/www -e js,mjs,json,ejs,html,js,css",
    "dev-default-admin-login": "NODE_ENV=dev DEV_LOGIN=admin DEBUG=awsconnectdashboard:* ./node_modules/nodemon/bin/nodemon.js ./bin/www -e js,mjs,json,ejs,html,js,css",
    "dev-default-non-admin-login": "NODE_ENV=dev DEV_LOGIN=nonadmin DEBUG=awsconnectdashboard:* ./node_modules/nodemon/bin/nodemon.js ./bin/www -e js,mjs,json,ejs,html,js,css",
    "uglifycss": "node utilities/uglifycss.js",
    "uglifyjs": "node utilities/uglifyjs.js",
    "pm-loc-start": "./node_modules/pm2/bin/pm2 start bin/www --name ccconfigpanel-loc --time --log logs/logs.txt",
    "pm-loc-stop": "./node_modules/pm2/bin/pm2 stop ccconfigpanel-loc",
    "pm-loc-logs": "./node_modules/pm2/bin/pm2 logs",
    "pm-loc-status": "./node_modules/pm2/bin/pm2 status",
    "pm-start": "pm2 start bin/www --name ccconfigpanel --time --log logs/logs.txt",
    "pm-stop": "pm2 stop ccconfigpanel"
  },
  "dependencies": {
    "@aws-sdk/client-connect": "^3.188.0",
    "@aws-sdk/client-dynamodb": "^3.190.0",
    "@aws-sdk/lib-dynamodb": "^3.190.0",
    "bcrypt": "^5.1.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "dynamoose": "^3.1.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-compression": "^1.0.2",
    "express-flash": "^0.0.2",
    "express-minify-html-2": "^1.0.1",
    "express-session": "^1.17.3",
    "group-by-with": "^1.0.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.2",
    "session-file-store": "^1.5.0",
    "set-interval-async": "^3.0.3",
    "socket.io": "^4.5.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "bootstrap": "^5.2.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "feather-icons": "^4.29.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "uglify-js": "^3.17.4",
    "uglifycss": "^0.0.29"
  }
}
